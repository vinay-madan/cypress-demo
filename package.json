{
  "name": "cypress-demo",
  "version": "1.0.0",
  "description": "Automate testing of sample e-commerce website using Cypress",
  "main": "index.js",
  "scripts": {
    "open:cypress": "npx cypress open",
    "run:tests": "npx cypress run --spec cypress/e2e/scenario.cy.js",
    "e2e:chrome": "cypress run --browser chrome",
    "run:tests:chrome": "npm run --headed e2e:chrome -- --spec cypress/e2e/scenario.cy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinay-madan/cypress-demo.git"
  },
  "keywords": [
    "cypress"
  ],
  "author": "vinay madan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vinay-madan/cypress-demo/issues"
  },
  "homepage": "https://github.com/vinay-madan/cypress-demo#readme",
  "devDependencies": {
    "@cypress/xpath": "^2.0.3",
    "cypress": "^13.15.0"
  }
}
